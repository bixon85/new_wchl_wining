type Vote = 
 record {
   fraudulent: bool;
   legitimate: bool;
 };
service : {
  addVote: (callId: text, fraudulent: bool, legitimate: bool) -> (text);
  checkVoteResult: (callId: text) -> (text);
  clearAllVotes: () -> (text);
  clearCallVotes: (callId: text) -> (text);
  getAllCallIds: () -> (vec text) query;
  getVotes: (callId: text) -> (opt vec Vote) query;
}
